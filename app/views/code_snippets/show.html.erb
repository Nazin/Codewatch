<h1><%= @code_snippet.title %></h1>

<div id="codeContainer">
	<table id="highlitedCode">
		<tbody>
		<tr>
			<td class="lineNumbers">
				<div>
					<%= raw gen_line_numbers @lines %>
				</div>
			</td>
			<td>
				<div>
					<%= raw @highlighted %>
				</div>
			</td>
		</tbody>
	</table>
</div>
