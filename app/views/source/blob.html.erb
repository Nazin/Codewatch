<h1>Filename:  <%= @blob.name.class %><%= @blob.name %></h1>
<h1>Id: <%= @id %></h1>
<% if @textfile %>
<table>
	<tbody>
		<tr>
			<td>
				<div>
					<%= raw gen_line_numbers @lines %>
					</div>
			</td>
			<td>
				<div>
					<%= raw @highlighted %>
				</div>
			</td>
	</tbody>
</table>
<% else %>
This file is probably not a text file. Preview not available.
<% end %>

